# Copy this file to .env and fill in the values
# Example ALchemy: https://eth-sepolia.g.alchemy.com/v2/your-key
# Example Infura: https://sepolia.infura.io/v3/your-key
SEPOLIA_URL="https://eth-sepolia.alchemyapi.io/v2/YOUR_ALCHEMY_KEY"
# Use the private key of the deployer account. Include the 0x prefix.
PRIVATE_KEY="0xYOUR_PRIVATE_KEY"
# Optional: Etherscan API key for contract verification on Sepolia
ETHERSCAN_API_KEY="your-etherscan-api-key"